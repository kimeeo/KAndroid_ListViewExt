<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SwipeDeck">
        <attr name="swipe_deck_max_visible" format="integer"/>
        <attr name="swipe_deck_rotation_degrees" format="float"/>
        <attr name="swipe_deck_card_spacing" format="dimension"/>
        <attr name="swipe_deck_render_above" format="boolean"/>
        <attr name="swipe_deck_render_below" format="boolean"/>
        <attr name="swipe_deck_opacity_end" format="float"/>
        <attr name="swipe_deck_card_gravity" format="enum">
            <enum name="top" value="0"/>
            <enum name="center" value="1"/>
            <enum name="bottom" value="2"/>
        </attr>
    </declare-styleable>
</resources>